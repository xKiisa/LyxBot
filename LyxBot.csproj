<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <RootNamespace>LyxBot</RootNamespace>
	  <AssemblyTitle>LyxBot</AssemblyTitle>
	  <Description>A small Discord Bot made in C#</Description>
    <AssemblyVersion>1.0.1</AssemblyVersion>
    <FileVersion>1.0.1</FileVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="dotenv.net" Version="3.2.0" />
    <PackageReference Include="DSharpPlus" Version="5.0.0-nightly-02317" />
    <PackageReference Include="DSharpPlus.Commands" Version="5.0.0-nightly-02317" />
    <PackageReference Include="DSharpPlus.Interactivity" Version="5.0.0-nightly-02317" />
  </ItemGroup>

</Project>
